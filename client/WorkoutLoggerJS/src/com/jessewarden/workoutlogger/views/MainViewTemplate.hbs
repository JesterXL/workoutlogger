<div xmlns="http://www.w3.org/1999/html">
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <ul class="list-group" id="workoutsList" name="workoutsList">
                    {{#each workouts}}
                        <li class="list-group-item"><a href="#" data="{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
            </div>
            <div class="col-md-8">
                <div id="workoutContent">Select a Workout</div>
                <div id="lastWorkoutContent"></div>
            </div>
        </div>

    </div>
</div>