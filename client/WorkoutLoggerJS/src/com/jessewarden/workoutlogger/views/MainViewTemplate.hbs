<div xmlns="http://www.w3.org/1999/html">
    <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
            Workout Type
            <span class="caret"></span>
        </button>
        <ul class="list-group" id="workoutsList" name="workoutsList">
        {{#each workouts}}
            <li class="list-group-item"><a href="#" data="{{id}}">{{name}}</a></li>
        {{/each}}
        </ul>
    </div>
</div>